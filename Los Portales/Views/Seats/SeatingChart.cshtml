
@{
    ViewData["Title"] = "SeatingChart";
}

<h1>SeatingChart</h1>
<div class="container-fluid" id="seatCon"> 
    @for(int i = 0; i < 8; i++)
    {
        
        <div class="row">
            @for(int j = 0; j < 10; j++)
            {
              <div class="col-sm-1">
                <svg width="100" height="100">
                    <rect  rx="10" ry="10" width="90" height="90"
                        style="fill:green;stroke:black;stroke-width:5;opacity:0.5" />
                    <text x="10" y="50" fill="black" font-weight="bold"> Hello </text>
                </svg> 
            </div>  
            }
        </div>       
    }
   
</div>

